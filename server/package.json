{
  "name": "brief-generator-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --tsconfig ./tsconfig.json --require tsconfig-paths/register index.ts",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate --config=./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/node": "^20.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nanoid": "^5.1.5",
    "openai": "^5.12.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "uuid": "^10.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/uuid": "^9.0.8",
    "drizzle-kit": "^0.31.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1"
  }
}
